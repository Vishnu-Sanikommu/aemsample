<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aemsampleuser"
        rep:privileges="{Name}[jcr:all]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aem-sample-service-user"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyAccessControl,jcr:read,jcr:readAccessControl,crx:replicate,rep:write,jcr:lockManagement]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aem-sample-service-user"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow2>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="vishnu"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:read,jcr:addChildNodes,jcr:lockManagement,jcr:nodeTypeManagement]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="vishnu"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aemsample-service-user"
        rep:privileges="{Name}[jcr:all]"/>
</jcr:root>
