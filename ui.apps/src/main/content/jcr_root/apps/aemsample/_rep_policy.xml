    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="vishnu"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:read,jcr:addChildNodes,jcr:lockManagement,jcr:nodeTypeManagement]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aemsample-service-user"
